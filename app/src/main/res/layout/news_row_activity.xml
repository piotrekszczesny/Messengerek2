<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/imageView_ostatniewiadomosci"
        android:layout_width="104dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView_ostatniewiadomosci"
        app:layout_constraintEnd_toStartOf="@+id/textView_ostatniewiadomosci"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/btn_star_big_on" />

    <TextView
        android:id="@+id/textView_ostatniewiadomosci"
        android:layout_width="306dp"
        android:layout_height="102dp"
        android:maxWidth="240dp"
        android:padding="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>